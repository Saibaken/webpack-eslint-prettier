{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
        "eqeqeq":"warn",
        "prettier/prettier": "error",
        "no-unused-vars": "error"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
    }
}
